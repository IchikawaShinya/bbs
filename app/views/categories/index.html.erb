<div class="category">
	<p id="notice"><%= notice %></p>
	
	<h1>Listing Categories</h1>
	
	<div class="categoryFooter">
		<%= link_to t("attributes.new_category"), new_category_path %>
	</div>
	
	<table class="category_list">
	  <tbody>
	    <% @categories.each do |category| %>
	      <tr>
	        <td><%= link_to category["category_name"], category %></td>
	      </tr>
	    <% end %>
	  </tbody>
	</table>
	<div class="categoryFooter">
		<%= link_to t("attributes.category_management"), categories_path+"/edit" %>
	</div>
</div>
